<!-- Plugin Configuration File. Read more: https://plugins.jetbrains.com/docs/intellij/plugin-configuration-file.html -->
<idea-plugin>
    <id>com.github.ydj515.stdnaminghound</id>
    <name>std-naming-hound</name>
    <vendor>ydj515</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.StdNamingHoundBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <notificationGroup
                id="StdNamingHound"
                displayType="BALLOON"
                isLogByDefault="false"
                title="Std Naming Hound"/>
        <toolWindow
                factoryClass="com.github.ydj515.stdnaminghound.toolWindow.StdNamingHoundToolWindowFactory"
                id="StdNamingHound"
                icon="/icons/stdNamingHound.svg"/>
        <postStartupActivity implementation="com.github.ydj515.stdnaminghound.startup.StdNamingHoundProjectActivity" />
        <applicationConfigurable
                displayName="Std Naming Hound"
                id="stdNamingHound.settings"
                instance="com.github.ydj515.stdnaminghound.settings.StdNamingHoundConfigurable"/>
    </extensions>
</idea-plugin>
